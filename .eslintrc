{
    "extends": [
        "airbnb",
        "plugin:jsx-a11y/recommended"
    ],
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "import/extensions": [1],
        "react/prefer-stateless-function": [1],
        "max-len": [1]
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    }
}